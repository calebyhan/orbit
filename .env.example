# Copy this file to `.env` and fill in your secrets. Do NOT commit your real keys.
# This file is automatically loaded by ORBIT - no need to manually export!

# Alpaca (news websocket)
ALPACA_API_KEY=your_alpaca_api_key
ALPACA_API_SECRET=your_alpaca_api_secret

# Reddit (OAuth)
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=ORBIT/1.0 by you@example.com

# Gemini API keys (support up to 5 keys for multi-key rotation)
GEMINI_API_KEY_1=your_gemini_key_1
GEMINI_API_KEY_2=
GEMINI_API_KEY_3=
GEMINI_API_KEY_4=
GEMINI_API_KEY_5=

# Runtime / environment
ORBIT_DATA_DIR=/srv/orbit/data
ORBIT_USER_AGENT=ORBIT/1.0
ORBIT_LOG_LEVEL=INFO

# Optional: override defaults
ORBIT_CONFIG_PATH=./config/default.yaml
